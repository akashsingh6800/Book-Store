<%- include('../includes/header.ejs') %>

</head>
<body>

    <%- include('../includes/navigation.ejs') %></body>

<main>
    <% if(orders.length<=0){%>
        <h1>Nothing There!</h1>
        <% } else{ %>
            <ul>
                
                    <% orders.forEach(order =>{ %>
                        <li>
                            <h1>
                               Order # <%= order._id%>
                            </h1>

                            <ul>
                                <% order.products.forEach(product=>{ %>
                                    <li> <%= product.productData.title%> ( <%= product.quantity%>)</li>
                               <% }) %>
                                
                            </ul>
                        </li>
                        

                   <% }) %>

               
            </ul>
            <% }%>


</main>


<%- include('../includes/end.ejs') %>
